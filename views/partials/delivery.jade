div(ng-if="!update.status")
  div(search-load-indicator)
div(ng-if="update.status")
  div(ng-if="update.status == 'pending'")
    .delivery-pending
        img.delivery-logo(src='data:image/jpeg;base64,', alt="postmates logo")
        h1 A courier is being assigned
  div(ng-show="update.status != 'pending'")
      div.delivery-update(ng-if="update.status == 'pickup'")
        img.delivery-driver(src='{{update.courier.img_href}}', alt='{{update.courier.name}}')
        p
          h1 {{update.courier.name}} is on their way to pickup your order!
      div.delivery-update(ng-if="update.status == 'dropoff'")
        img.delivery-logo(src='{{update.courier.img_href}}', alt='{{update.courier.name}}')
        p
          h1 {{update.courier.name}} is on their way to you!
      div.delivery-update(ng-if="update.status == 'delivered'")  
        h1 Delivery Complete

    #delivery-map
